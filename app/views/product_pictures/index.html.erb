<h1>Listing product_pictures</h1>

<table>
  <tr>
    <th>User</th>
    <th>Catalog</th>
    <th>Product</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_pictures.each do |product_picture| %>
  <tr>
    <td><%= product_picture.user_id %></td>
    <td><%= product_picture.catalog_id %></td>
    <td><%= product_picture.product_id %></td>
    <td><%= product_picture.note %></td>
    <td><%= link_to 'Show', product_picture %></td>
    <td><%= link_to 'Edit', edit_product_picture_path(product_picture) %></td>
    <td><%= link_to 'Destroy', product_picture, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Product picture', new_product_picture_path %>
